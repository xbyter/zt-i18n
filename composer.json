{
  "name": "ztphp/zt-i18n",
  "description": "i18n客户端",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "Translator",
    "PHP"
  ],
  "require": {
    "php": "^7.0 || ^8.0",
    "xbyter/apollo-client": "^1.0",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5.6 || ^7.0 || ^9.6",
    "illuminate/console": "^5.5 || ^6.0",
    "illuminate/routing": "^5.5 || ^6.0",
    "illuminate/container": "^5.5 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "ZtI18n\\": "src/"
    },
    "files":["src/Laravel/function.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "ZtI18nTests\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "ZtI18n\\Laravel\\Providers\\I18nServiceProvider"
      ]
    }
  }
}
